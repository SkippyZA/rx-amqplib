{
  "name": "rx-amqplib",
  "version": "0.1.3",
  "description": "RxJS wrapper for the amqplib from squaremo",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/SkippyZA/rx-amqplib.git"
  },
  "keywords": [
    "rxjs",
    "amqp",
    "amqplib",
    "rabbitmq",
    "queue"
  ],
  "author": "Steven Inskip",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SkippyZA/rx-amqplib/issues"
  },
  "main": "src/RxAmqpLib.ts",
  "scripts": {
    "clean": "node_modules/.bin/rimraf dist/*",
    "compile:ts": "node_modules/.bin/tsc",
    "build:lint": "node_modules/.bin/tslint --config=tslint.json --project=tsconfig.json",
    "prebuild": "npm run clean && npm run build:lint",
    "build": "npm run prebuild && npm run compile:ts",


    "example-client": "gulp && node ./examples/client.js",
    "example-emit-logs-direct": "gulp && node ./examples/emit_log_direct.js",
    "example-emit-logs-topic": "gulp && node ./examples/emit_log_topic.js",
    "example-emit-logs": "gulp && node ./examples/emit_logs.js",
    "example-new-task": "gulp && node ./examples/new_task.js",
    "example-receive-logs": "gulp && node ./examples/receive_logs.js",
    "example-receive-logs-direct": "gulp && node ./examples/receive_logs_direct.js",
    "example-receive-logs-topic": "gulp && node ./examples/receive_logs_topic.js",
    "example-rcp-client": "gulp && node ./examples/rpc_client.js",
    "example-rcp-server": "gulp && node ./examples/rpc_server.js",
    "example-server": "gulp && node ./examples/server.js",
    "example-worker": "gulp && node ./examples/worker.js"
  },
  "homepage": "https://github.com/SkippyZA/rx-amqplib#readme",
  "dependencies": {
    "amqplib": "^0.4.1",
    "node-uuid": "^1.4.7",
    "rx": "^4.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "rimraf": "^2.5.4",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3"
  }
}
